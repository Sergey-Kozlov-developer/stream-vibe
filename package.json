{
  "name": "minista-project",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "minista",
    "build": "minista build",
    "preview": "minista preview"
  },
  "prettier": {
    "semi": true,
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "bracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": true,
    "vueIndentScriptAndStyle": false,
    "embeddedLanguageFormatting": "auto"
  },
  "devDependencies": {
    "minista": "^3.1.12",
    "postcss-pxtorem": "^6.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.81.0"
  },
  "dependencies": {
    "@a1rth/css-normalize": "^1.1.9",
    "classnames": "^2.5.1",
    "imask": "^7.6.1",
    "swiper": "^11.1.15"
  }
}
